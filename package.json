{
  "name": "dtft",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others --names \"NODE,REACT\" -c \"bgGreen.black,bgCyan.black\" \"nodemon ./server.js\" \"vite build --watch --mode development\"",
    "start": "npm run build && npm run prod:api",
    "dev:api": "nodemon ./server.js",
    "prod:api": "node ./server.js",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "dev:ui": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongodb": "^4.13.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.6.1",
    "react-scroll": "^1.8.9",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "concurrently": "^7.6.0",
    "eslint-config-prettier": "^8.6.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "vite": "^4.0.4"
  }
}
